{
  "git": {
    "deploymentEnabled": {
      "main": false,
      "clarissa": true
    }
  },
  "github": {
    "enabled": true,
    "autoAlias": false
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "domains": [
    "clini.one",
    "www.clini.one",
    "*.clini.one"
  ]
}